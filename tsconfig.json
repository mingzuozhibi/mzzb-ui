{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "#CA/*": ["src/commons/app/*"],
      "#CC/*": ["src/commons/comps/*"],
      "#CH/*": ["src/commons/hooks/*"],
      "#CU/*": ["src/commons/utils/*"],

      "#DF/*": ["src/domains/feats/*"],
      "#DT/*": ["src/domains/types/*"],
      "#DU/*": ["src/domains/utils/*"],

      "#RC/*": ["src/routers/comps/*"],
      "#RP/*": ["src/routers/pages/*"],
      "#RU/*": ["src/routers/utils/*"]
    }
  },
  "include": ["src"]
}
