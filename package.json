{
  "private": true,
  "name": "mzzb-ui",
  "version": "5.3.0",
  "license": "Apache-2.0",
  "description": "ui for mingzuozhibi",
  "proxy": "https://mingzuozhibi.com",
  "#proxy": "http://localhost:9000",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "eject": "react-scripts eject",
    "size": "source-map-explorer 'build/static/js/*.js' --html >~/downs/size.html",
    "c1": "sed -i 's/http:/https:/g' yarn.lock",
    "c2": "sed -i 's/mirrors.cloud.tencent.com/mirrors.tencent.com/g' yarn.lock",
    "ca": "yarn c1 && yarn c2",
    "t1": "sed -i '7,8s/#proxy/#temp/g' package.json",
    "t2": "sed -i '7,8s/proxy/#proxy/g' package.json",
    "t3": "sed -i '7,8s/#temp/proxy/g' package.json",
    "ta": "yarn t1 && yarn t2 && yarn t3"
  },
  "dependencies": {
    "@ahooksjs/use-url-state": "^3.5.0",
    "@ant-design/icons": "^4.7.0",
    "@reduxjs/toolkit": "^1.8.6",
    "ahooks": "^3.7.1",
    "antd": "^4.23.5",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.5",
    "echarts": "^5.4.0",
    "immer": "^9.0.15",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.186",
    "@types/md5": "^2.3.2",
    "@types/node": "^16.11.65",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "customize-cra": "^1.0.0",
    "react-app-rewired": "^2.2.1",
    "sass": "^1.55.0",
    "source-map-explorer": "^2.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mingzuozhibi/mzzb-ui.git"
  },
  "bugs": {
    "url": "https://github.com/mingzuozhibi/mzzb-ui/issues",
    "email": "mzzb_website@126.com"
  },
  "author": {
    "name": "fuhaiwei",
    "email": "fuhaiwei@126.com"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
